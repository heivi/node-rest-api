    "cookie-parser": "*",
    "debug": "^2.2.0",
    "express": "^5.0.0-alpha.2",
    "faker": "^3.0.0",
    "method-override": "*",
    "mongoose": "^4.0.8",
    "nconf": "^0.7.1",
    "oauth2orize": "*",
    "passport": "*",
    "passport-http": "*",
    "passport-http-bearer": "*",
